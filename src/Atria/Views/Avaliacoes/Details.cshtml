@model Atria.Models.Avaliacao
@{
    ViewData["Title"] = "Detalhes da Avaliação";
}
<h1>Detalhes</h1>
<dl class="row">
    <dt class="col-sm-2">Nota</dt>
    <dd class="col-sm-10">@Model.Nota</dd>
    <dt class="col-sm-2">Tipo</dt>
    <dd class="col-sm-10">@Model.TipoAvaliacao</dd>
    <dt class="col-sm-2">Usuário</dt>
    <dd class="col-sm-10">@Model.Usuario?.Nome</dd>
    <dt class="col-sm-2">Material</dt>
    <dd class="col-sm-10">@Model.Material?.Titulo</dd>
    <dt class="col-sm-2">Resenha</dt>
    <dd class="col-sm-10">@Model.Resenha</dd>
</dl>
<p><a class="btn btn-secondary" asp-action="Index">Voltar</a></p>